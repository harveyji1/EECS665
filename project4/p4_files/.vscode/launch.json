{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug dmc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dmc", // Set the path to your executable
            "args": [
                "p4_tests2/conditionalTest.dm",
                "-n",
                "p4_tests2/conditionalTest.out"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/Repository/EECS665/project4/p4_files": "${workspaceFolder}"
            },
            "logging": {
                "engineLogging": true
            }
        }

    ]
}